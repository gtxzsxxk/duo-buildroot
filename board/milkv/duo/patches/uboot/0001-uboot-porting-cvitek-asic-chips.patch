+	.get_cd	= cvi_get_cd,
+#ifdef CONFIG_MMC_SUPPORTS_TUNING
+	.platform_execute_tuning = cvi_general_execute_tuning,
+#endif
+#ifdef CONFIG_MMC_UHS_SUPPORT
+	.voltage_switch = cvi_sd_voltage_switch,
+#endif
+	.reset = cvi_general_reset,
+};
