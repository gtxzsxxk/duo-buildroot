+#define DESC_TXSTS_NO_ERR		(0x0 << 28)
+#define DESC_TXSTS_BUFFFLUSH		(0x1 << 28)
+#define DESC_TXSTS_BUF_ERR		(0x3 << 28)
+/* varies on isochronous in*/
