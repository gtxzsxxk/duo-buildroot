+int cvi_get_cd(struct sdhci_host *host)
+{
+	u32 reg;
+
+	reg = sdhci_readl(host, SDHCI_PRESENT_STATE);
+	pr_debug("%s reg = 0x08%x\n", __func__, reg);
+	if (reg & SDHCI_CARD_PRESENT) {
+		return 1;
+	} else {
+		return 0;
+	}
+}
