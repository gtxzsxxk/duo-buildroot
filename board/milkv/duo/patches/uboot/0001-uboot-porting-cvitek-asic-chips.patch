+	.id = UCLASS_MMC,
+	.of_match = cvi_sdhci_match,
+	.of_to_plat = cvi_ofdata_to_platdata,
+	.bind = cvi_sdhci_bind,
+	.probe = cvi_sdhci_probe,
+	.priv_auto = sizeof(struct cvi_sdhci_host),
+	.plat_auto = sizeof(struct cvi_sdhci_plat),
+	.ops = &sdhci_ops,
+};
