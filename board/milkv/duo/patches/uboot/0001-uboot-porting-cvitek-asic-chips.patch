+	value = mmio_read_32(TOP_BASE + REG_TOP_SOFT_RST) & (~BIT_TOP_SOFT_RST_USB);
+	mmio_write_32(TOP_BASE + REG_TOP_SOFT_RST, value);
+	udelay(50);
+	value = mmio_read_32(TOP_BASE + REG_TOP_SOFT_RST) | BIT_TOP_SOFT_RST_USB;
+	mmio_write_32(TOP_BASE + REG_TOP_SOFT_RST, value);
