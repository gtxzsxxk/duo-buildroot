+static uint32_t spi_nand_crc16_ccitt_with_tag(unsigned char *buf, int len)
+{
+	uint32_t crc = 0;
+
+	crc = crc16_ccitt(0, buf, len);
+	crc |= 0xCAFE0000;
+
+	return crc;
