+		mmio_clrsetbits_32(REG_SDIO0_DAT0_PAD_REG, REG_SDIO0_PAD_CLR_MASK,
+				   REG_SDIO0_DAT0_PAD_RESET << REG_SDIO0_PAD_SHIFT);
+
+		mmio_clrsetbits_32(REG_SDIO0_DAT2_PAD_REG, REG_SDIO0_PAD_CLR_MASK,
