+	mmio_write_32(PINMUX_USB_VBUS_DET, 0);
+	vbus = (!!(mmio_read_32(REG_TOP_CONF_INFO) & BIT_TOP_CONF_INFO_VBUS));
+	mmio_write_32(PINMUX_USB_VBUS_DET, save_pinmux);
