+/*****************************************************************************/
+static u16 cvsnfc_read_word(struct mtd_info *mtd)
+{
+	struct nand_chip *chip = mtd->priv;
+	struct cvsnfc_host *host = chip->priv;
+
+	host->offset += 2;
+	return readw(host->buffer + host->column + host->offset - 2);
+}
