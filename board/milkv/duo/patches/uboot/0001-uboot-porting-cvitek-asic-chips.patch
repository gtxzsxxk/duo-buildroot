+	uint64_t target = sram_info.sram_dest + req->length;
+	/* INFO("sram copy data to 0x%lx, len = 0x%x\n", sram_info.sram_dest, req->length); */
+	memcpy((void *)sram_info.sram_dest, (void *)req->buf, req->length);
+
+	if (left == 0U)
+		resetOutReq();
+	else
+		sramOutReqS2D(target, left);
+}
