+void  cvsnfc_setup_intr(struct cvsnfc_host *host)
+{
+	cvsfc_write(host, REG_SPI_NAND_INT_EN, 0x1f1);
+	cvsfc_write(host, REG_SPI_NAND_INT_CLR, BITS_SPI_NAND_INT_CLR_ALL);
+	cvsfc_write(host, REG_SPI_NAND_INT_MASK, 0);
+}
