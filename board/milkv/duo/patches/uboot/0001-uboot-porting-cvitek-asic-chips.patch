+static inline unsigned int ANY_BP_ENABLE(unsigned int val)
+{
+	return ((PROTECTION_BP3_MASK & (val)) || (PROTECTION_BP2_MASK & (val)) ||
+		(PROTECTION_BP1_MASK & (val)) || (PROTECTION_BP0_MASK & (val)));
+}
