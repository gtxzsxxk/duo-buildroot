+def crc16_ccitt(HexData):
+    cksum = int("0", 16)
+    for i in range(len(HexData)):
+        cksum = crc16_tab[((cksum >> 8) ^ HexData[i]) & 0xff] ^ ((cksum << 8) & 0xff00)
